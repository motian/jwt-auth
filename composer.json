{
    "name": "motian/jwt-auth",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "authors": [
        {
            "name": "Li Yuzhao",
            "email": "562405704@qq.com",
            "homepage": "https://github.com/phper666/jwt-auth",
            "role": "Developer"
        },
        {
            "name": "Wence Xie",
            "email": "wence.xie@gmail.com",
            "homepage": "https://github.com/motian/jwt-auth",
            "role": "Developer"
        }
    ],
    "description": "jwt-auth Component",
    "autoload": {
        "psr-4": {
            "Phper666\\JwtAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=7.2",
        "ext-swoole": ">=4.5",
        "hyperf/utils": "~2.0.0",
        "hyperf/framework": "~2.0.0",
        "hyperf/di": "~2.0.0",
        "hyperf/cache": "~2.0.0",
        "lcobucci/jwt": "^3.2",
        "nesbot/carbon": "^1.0 || ^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "hyperf/testing": "~2.0.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^0.12",
        "swoole/ide-helper": "^4.5"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "co-phpunit -c phpunit.xml --colors=always",
        "analyze": "phpstan analyse --memory-limit 300M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "Phper666\\JwtAuth\\ConfigProvider"
        }
    }
}
